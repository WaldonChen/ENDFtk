#ifndef ENDFTK_IMPLEMENTATION_FILESKELETON_SRC_OPERATOR_HPP
#define ENDFTK_IMPLEMENTATION_FILESKELETON_SRC_OPERATOR_HPP

namespace ENDFtk {
namespace implementation {

/** @brief Return the contained SectionSkeleton given an @param MT identifier.
 *
 * This throws an exception if there is no Section for the given MT.
 */
template<typename Iterator>
const SectionSkeleton<Iterator>& 
FileSkeleton<Iterator>::operator[]( int MT ){
  
  try{
    return sections_.at(MT);
  } catch( std::out_of_range& o ){
    LOG(ERROR) << "There is no MT=" << MT << " in MF=" << MF_;
    throw o;
  } 
}

} // namespace implementation
} // namespace ENDFtk

#endif  // ENDFTK_IMPLEMENTATION_FILESKELETON_SRC_OPERATOR_HPP

